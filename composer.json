{
  "name": "codetechnl/route-interface",
  "description": "A web-based route list GUI for Laravel.",
  "keywords": [
    "laravel",
    "router",
    "interface",
    "development"
  ],
  "authors": [
    {
      "name": "Donny van Grondelle",
      "email": "info@codetech.nl"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": ">=5.6"
  },
  "require-dev": {
    "orchestra/testbench": ">=3.6",
    "phpunit/phpunit": "^7.0|^8.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "CodeTech\\RouteInterface\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CodeTech\\RouteInterface\\RouteInterfaceServiceProvider"
      ]
    }
  }
}
